farming:AddToggle("Light Karma Farm", function()
  if settings.lightkarmaautofarm == false then
    settings.lightkarmaautofarm = true
    workspace.Dddyunknown_45273.HumanoidRootPart.CFrame = CFrame.new(330.128, 30432.824, -16.131)
    while wait() do
